{
  "version": 2,
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, min-fresh=5, max-stale[=5], no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "build": {
    "env": {
      "PRISMIC_ACCESS_TOKEN": "@prismic-access-token"
    }
  }
}
